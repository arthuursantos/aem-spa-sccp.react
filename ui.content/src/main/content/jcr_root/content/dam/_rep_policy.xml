<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:crx="http://www.day.com/crx/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="everyone"
        rep:privileges="{Name}[jcr:removeNode]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:current="[]"/>
    </deny>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="scene7-config-writer-service"
        rep:privileges="{Name}[jcr:read,rep:write]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-replication-service"
        rep:privileges="{Name}[jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write,crx:replicate]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-update-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write]"/>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="webdavbkpservice"
        rep:privileges="{Name}[jcr:all]"/>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dynamicmedia-asset-service"
        rep:privileges="{Name}[jcr:read]"/>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dam-users"
        rep:privileges="{Name}[jcr:versionManagement,rep:write,crx:replicate,jcr:lockManagement]"/>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="campaign-remote"
        rep:privileges="{Name}[crx:replicate]"/>
</jcr:root>
